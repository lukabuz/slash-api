{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"resolveJsonModule": true,
		"noImplicitAny": false,
		"moduleResolution": "node"
	},
	"compileOnSave": true,
	"include": [
		"src"
	]
}